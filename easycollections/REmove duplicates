import java.io.*;
import java.util.*;

public class Solution {

    public static void main(String[] args) {
        Scanner input = new Scanner(System.in);
        
        String str = input.next();
        LinkedHashSet<Character> set = new LinkedHashSet<>();
        for(int i=0;i<str.length();i++){
            char c = str.charAt(i);
            if(!set.contains(c)){
                set.add(c);
            }
        }
        
        for(Character c:set){
            System.out.print(c);
        }
    }
}
